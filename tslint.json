{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "ordered-imports": false,
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "interface-name": [
      false, "never-prefix"
    ],
    "no-console": false,
    "prefer-const": false,
    "interface-over-type-literal": false,
    "max-line-length": false
  },
  "rulesDirectory": []
}
